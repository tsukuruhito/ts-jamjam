{
	"name": "ts-jamjam",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"check-types": "tsc --noEmit",
		"format": "prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json}\"",
		"prepare": "husky install",
		"pre-commit": "lint-staged",
		"test": "lint-staged"
	},
	"lint-staged": {
		"./src/**/*.{js,jsx,ts,tsx}": [
			"git add .",
			"next lint",
			"prettier --write --ignore-path .gitignore \"./**/*.{js,jsx,ts,tsx,json}\""
		]
	},
	"dependencies": {
		"next": "12.2.5",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@types/node": "18.7.9",
		"@types/react": "18.0.17",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "^10.4.8",
		"eslint": "8.22.0",
		"eslint-config-next": "12.2.5",
		"eslint-config-prettier": "^8.5.0",
		"lint-staged": "^13.0.3",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"tailwindcss": "^3.1.8",
		"typescript": "4.7.4",
		"husky": "^8.0.0"
	}
}
